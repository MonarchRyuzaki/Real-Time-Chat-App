{
  "name": "real-time-chat-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "dev:watch": "ts-node --watch src/index.ts",
    "clean": "rm -rf dist",
    "client": "ts-node client.ts",
    "test-client": "ts-node test-client.ts",
    "group-test-client": "ts-node group-test-client.ts",
    "presence-test-client": "ts-node presence-test-client.ts",
    "docker:build": "docker compose build && docker image prune -f",
    "docker:up": "docker compose up -d --build && docker image prune -f",
    "docker:down": "docker compose down",
    "db:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "db:push": "prisma db push --schema=src/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=src/prisma/schema.prisma",
    "db:seed": "ts-node src/prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "bcrypt": "^6.0.0",
    "cassandra-driver": "^4.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "redis": "^5.6.0",
    "winston": "^3.17.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^24.0.13",
    "@types/ws": "^8.5.13",
    "nodemon": "^3.1.7",
    "prisma": "^6.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
